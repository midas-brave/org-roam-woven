# org-roam-woven Environment Configuration
# Copy this file to .env and modify the values as needed

# GitHub repository (automatically set in CI/CD)
GITHUB_REPOSITORY=your-org/org-roam-woven

# Docker image tag
TAG=latest

# Application configuration
PORT=18080
GIN_MODE=release

# Docker Compose project name
COMPOSE_PROJECT_NAME=org-roam-woven

# Data directory for org-roam files
ORG_ROAM_DATA_PATH=./data
